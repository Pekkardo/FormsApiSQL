create database farmacia_TP
go
use farmacia_TP
go
set dateformat dmy;
CREATE TABLE provincias (
    id INT identity,
    nombre_provincia VARCHAR(50)
	constraint pk_provincias primary key (id)
)
CREATE TABLE localidades (
    id INT identity,
    nombre_localidad VARCHAR(50),
    id_provincia INT,
    CONSTRAINT pk_localidades PRIMARY KEY (id),
    CONSTRAINT fk_Localidades_Provincias FOREIGN KEY (id_provincia) REFERENCES Provincias(id)
) 
CREATE TABLE condiciones_IVA (
    id INT identity,
    nombre_condicion VARCHAR(50)
	constraint pk_condiciones_IVA primary key(id)
)
CREATE TABLE tipos_documentos (
    id INT identity,
    nombre_tipo VARCHAR(50)
	constraint pk_tipos_documentos primary key (id)
)
CREATE TABLE clientes (
    id INT identity,
    DNI INT,
    Nombre VARCHAR(50),
    Apellido VARCHAR(50),
    id_condicion INT,
    id_tipo_documento INT, -- Nueva columna para el tipo de documento
	calle varchar(100),
	nro_calle int,
	id_localidad int,
	constraint pk_clientes primary key (id),
    constraint fk_clientes_condiciones_iva FOREIGN KEY (id_condicion) 
	REFERENCES Condiciones_IVA(id),
    constraint fk_clientes_tipos_documentos FOREIGN KEY (id_tipo_documento) 
	REFERENCES tipos_documentos(id),
	constraint fk_clientes_localidades foreign key (id_localidad)
	references localidades (id)
)
CREATE TABLE sucursales
(
id int identity,
descripcion varchar(50),
direccion varchar(100),
id_localidad int
CONSTRAINT pk_sucursales PRIMARY KEY(id),
CONSTRAINT fk_sucursales_localidades FOREIGN KEY (id_localidad) 
REFERENCES localidades(id)
)
create table tipo_empleados (
id int identity,
descripcion varchar(100)
constraint pk_tipo_empleados primary key (id)
)

create table empleados(
legajo int identity,
id_tipo_empleado int,
nombre varchar(50),
apellido varchar(50),
fecha_nac datetime,
fecha_alta datetime,
id_sucursal int
constraint pk_empleados primary key (legajo),
constraint fk_tipo_empleado foreign key (id_tipo_empleado)
references tipo_empleados(id),
constraint fk_id_sucursal foreign key (id_sucursal)
references sucursales (id)
)
create table tipos_suministros(
id int,
nombre varchar(50)
constraint pk_tipos_suministros primary key(id)
)
create table tipos_ventas(
id int,
descripcion varchar(100)
constraint pk_tipos_ventas primary key (id)
)
create table suministros (
id int,
descripcion varchar(100),
codigo_barra bigint,
id_tipo int,
id_tipo_venta int,
precio float
constraint pk_suministros primary key (id),
constraint fk_suministros_tipos_suministros foreign key (id_tipo)
references tipos_suministros(id),
constraint fk_suministros_tipos_ventas foreign key (id_tipo_venta)
references tipos_ventas(id)
)
CREATE TABLE stocks (
id_sucursal int,
id_suministro int,
stock int
CONSTRAINT pk_stocks PRIMARY KEY(id_sucursal, id_suministro),
CONSTRAINT fk_stocks_sucursales FOREIGN KEY (id_sucursal) 
REFERENCES sucursales(id),
CONSTRAINT fk_stocks_suministros FOREIGN KEY (id_suministro) 
REFERENCES suministros(id)
)
CREATE TABLE recetas (
    id INT identity,
    paciente VARCHAR(100),
    DNI INT,
    medico VARCHAR(100),
    fecha datetime,
    id_suministro INT,
    CONSTRAINT pk_recetas PRIMARY KEY (id),
    CONSTRAINT fk_recetas_suministros FOREIGN KEY (id_suministro) 
	REFERENCES suministros(id)
)
CREATE TABLE obras_sociales
(
id int IDENTITY,
nombre Varchar(50),
id_condicionIVA int,
cuit bigint,
CONSTRAINT pk_obras_sociales PRIMARY KEY(id),
CONSTRAINT fk_obras_sociales_condicionIVA FOREIGN KEY (id_condicionIVA) 
REFERENCES condiciones_IVA(id)
)
CREATE TABLE Proveedores
(
id int identity,
nombre varchar(100),
direccion varchar(100),
id_localidad int,
cuit bigint
CONSTRAINT pk_proveedores PRIMARY KEY(id),
CONSTRAINT fk_proveedores_localidades FOREIGN KEY (id_localidad) 
REFERENCES localidades(id)
)
CREATE TABLE tipos_contacto
(
id int identity,
descripcion varchar(50)
CONSTRAINT pk_tipos_contacto PRIMARY KEY(id)
)
CREATE TABLE Contactos (
id int identity,
id_tipos_contacto int,
descripcion varchar(100),
id_obra int,
id_sucursal int,
id_cliente int,
id_empleado int,
id_proveedor int
CONSTRAINT pk_contactos PRIMARY KEY(id),
CONSTRAINT fk_contactos_tiposcontacto FOREIGN KEY (id_tipos_contacto) 
REFERENCES tipos_contacto(id),
CONSTRAINT fk_contactos_obrassociales FOREIGN KEY (id_obra) 
REFERENCES obras_sociales(id),
CONSTRAINT fk_contactos_sucursales FOREIGN KEY (id_sucursal) 
REFERENCES sucursales(id),
CONSTRAINT fk_contactos_clientes FOREIGN KEY (id_cliente) 
REFERENCES clientes(id),
CONSTRAINT fk_contactos_empleados FOREIGN KEY (id_empleado) 
REFERENCES empleados(legajo),
CONSTRAINT fk_contactos_proveedores FOREIGN KEY (id_proveedor) 
REFERENCES Proveedores(id)
)
CREATE TABLE pedidos (
id int identity,
id_sucursal int,
id_proveedor int,
id_empleado int,
transportista varchar (100),
fecha  datetime,
constraint id_pedidos PRIMARY KEY (id),
constraint fk_pedidos_sucursal FOREIGN KEY (id_sucursal) 
references Sucursales (id),
constraint fk_pedidos_proveedor FOREIGN KEY (id_proveedor) 
references Proveedores (id),
constraint fk_pedidos_id_empleado FOREIGN KEY (id_empleado) 
references Empleados (legajo)
) 

CREATE TABLE detalles_pedidos (
id int identity,
id_pedidos int,
id_suministro int,
cantidad int
constraint id_detalles_pedidos PRIMARY KEY (id),
constraint fk_detalles_pedidos_pedidos FOREIGN KEY (id_pedidos) 
references pedidos (id),
constraint fk_detalles_pedidos_suministros FOREIGN KEY (id_suministro) 
references suministros (id)
)

CREATE TABLE descuentos (
id int identity,
id_obra int,
id_localidad int,
id_suministro int,
descuento int
CONSTRAINT pk_descuentos PRIMARY KEY(id),
CONSTRAINT fk_descuentos_obrasociales FOREIGN KEY (id_obra) 
REFERENCES obras_sociales(id),
CONSTRAINT fk_descuentos_localidades FOREIGN KEY (id_localidad) 
REFERENCES localidades(id),
CONSTRAINT fk_descuentos_suministros FOREIGN KEY (id_suministro) 
REFERENCES suministros(id)
)
create table medios_pagos (
id int identity,
descripcion varchar(100),
recargo float, 
constraint id_medio_pagos PRIMARY KEY (id)
)
CREATE TABLE facturas (
numero int identity,
fecha datetime,
id_cliente int,
id_sucursal int,
id_empleado int,
pagado bit
CONSTRAINT pk_facturas PRIMARY KEY(numero),
CONSTRAINT fk_facturas_clientes FOREIGN KEY (id_cliente) 
REFERENCES clientes(id),
CONSTRAINT fk_facturas_sucursal FOREIGN KEY (id_sucursal) 
REFERENCES sucursales(id),
CONSTRAINT fk_facturas_empleado FOREIGN KEY (id_empleado) 
REFERENCES empleados(legajo),
)
create table medios_pagos_X_factura(
id int identity,
id_medio_pago int,
id_factura int,
cuotas int
constraint id_medios_pagos_X_factura PRIMARY KEY (id)
constraint fk_medios_pagos_X_factura_medio_pago FOREIGN KEY (id_medio_pago) 
references medios_pagos (id),
constraint fk_medios_pagos_X_factura FOREIGN KEY (id_factura) 
references facturas (numero)
) 
CREATE TABLE detalles_facturas
(
id int identity,
id_factura int,
id_suministro int,
id_descuento int,
cantidad int,
precio float
CONSTRAINT pk_detalles_facturas PRIMARY KEY(id),
CONSTRAINT fk_detalles_facturas FOREIGN KEY (id_factura) 
REFERENCES facturas(numero),
CONSTRAINT fk_detalles_suministros FOREIGN KEY (id_suministro) 
REFERENCES suministros(id),
CONSTRAINT fk_detalles_descuento FOREIGN KEY (id_descuento) 
REFERENCES descuentos(id),
)

